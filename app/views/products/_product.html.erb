<div id="<%= dom_id product %>">
  <p>
    <strong>Brand:</strong>
    <%= product.brand.title %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= product.title %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Img:</strong>
    <%= image_tag(product.img, alt: "",size: "160x100", class: "img-responsive zoom-img") %>
  </p>

</div>
